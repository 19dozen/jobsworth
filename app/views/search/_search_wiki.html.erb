<tr id="wiki_<%=@page.id%>">
<td class="wiki-search">
  <%= "<a href=\"/wiki/show/#{URI.encode(@page.name)}\">".html_safe + highlight_all(@page.name, @keys || []) + "</a>".html_safe %>
  <%#= link_to highlight_all(h(@page.name), @keys || []), :controller => "wiki", :action => "show", :id => URI.encode(@page.name) %>
</td>
<td class="wiki-search-body"><%= highlight_all(excerpt(@page.body, @keys[0]), @keys || []) %></td>
</tr>
